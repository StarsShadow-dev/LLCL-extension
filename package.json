{
	"name": "parallel",
	"displayName": "Parallel-lang VS Code Extension",
	"description": "extension for VS Code",
	"version": "0.0.1",
	"publisher": "starsshadowdev",
	"repository": "https://github.com/StarsShadow-dev/Parallel-lang-extension",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "parallel",
				"extensions": [
					".parallel"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "parallel",
				"scopeName": "source.parallel",
				"path": "./grammar.json",
				"injectTo": [
					"parallel"
				]
			}
		],
		"snippets": [
            {
                "language": "parallel",
                "path": "./snippets.json"
            }
        ]
	}
}